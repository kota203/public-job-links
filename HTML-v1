                    {"title": "Farmworkers and Laborers, Crop, Nursery, and Greenhouse", "min_salary": 22000, "max_salary": 38000},
                    {"title": "Graders and Sorters, Agricultural Products", "min_salary": 21000, "max_salary": 36000},
                    {"title": "Fishing and Hunting Workers", "min_salary": 22000, "max_salary": 45000}
                ]
            }
        };

        // Script to render the data
        const container = document.getElementById('jobs-container');
        const formatter = new Intl.NumberFormat('en-US', {
            style: 'currency',
            currency: 'USD',
            minimumFractionDigits: 0
        });

        for (const sector in jobData) {
            const sectorDiv = document.createElement('div');
            sectorDiv.className = 'sector';

            const sectorTitle = document.createElement('h2');
            sectorTitle.textContent = `${sector} (NAICS ${jobData[sector].code})`;
            sectorDiv.appendChild(sectorTitle);

            const jobList = document.createElement('ul');
            jobData[sector].jobs.forEach(job => {
                const listItem = document.createElement('li');
                
                // Create a container for the job title and salary
                const jobInfoDiv = document.createElement('div');
                jobInfoDiv.className = 'job-info';

                // Create and append the job link
                const link = document.createElement('a');
                const query = job.title.replace(/ /g, '+');
                link.href = `https://www.indeed.com/jobs?q=${query}`;
                link.textContent = job.title;
                link.target = '_blank';
                jobInfoDiv.appendChild(link);

                // Create and append the salary span
                const salarySpan = document.createElement('span');
                salarySpan.className = 'salary';
                salarySpan.textContent = `Est. Annual Salary: ${formatter.format(job.min_salary)} - ${formatter.format(job.max_salary)}`;
                jobInfoDiv.appendChild(salarySpan);

                listItem.appendChild(jobInfoDiv);

                // Create a container for the checkboxes
                const checkboxesDiv = document.createElement('div');
                checkboxesDiv.className = 'checkbox-container';

                // Add "Applied:" label
                const appliedLabel = document.createElement('span');
                appliedLabel.className = 'applied-label';
                appliedLabel.textContent = 'Applied:';
                checkboxesDiv.appendChild(appliedLabel);

                // Create 10 checkboxes
                for (let i = 0; i < 10; i++) {
                    const checkbox = document.createElement('input');
                    checkbox.type = 'checkbox';
                    
                    // Sanitize the job title to create a valid ID
                    const sanitizedTitle = job.title.replace(/[^a-zA-Z0-9]/g, '');
                    checkbox.id = `naics${jobData[sector].code}-${sanitizedTitle}-box${i}`;

                    // Load saved state from localStorage
                    if (localStorage.getItem(checkbox.id) === 'true') {
                        checkbox.checked = true;
                    }

                    // Add event listener to save state on change
                    checkbox.addEventListener('change', function() {
                        localStorage.setItem(this.id, this.checked);
                    });

                    checkboxesDiv.appendChild(checkbox);
                }
                
                listItem.appendChild(checkboxesDiv);
                jobList.appendChild(listItem);
            });

            sectorDiv.appendChild(jobList);
            container.appendChild(sectorDiv);
        }
    </script>
</body>
</html>
